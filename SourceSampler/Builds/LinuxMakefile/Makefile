clean:
	@echo Cleaning SourceSampler
	$(V_AT)$(CLEANCMD)

strip:
	@echo Stripping SourceSampler
	-$(V_AT)$(STRIP) --strip-unneeded $(JUCE_OUTDIR)/$(TARGET)

