#!/bin/bash
# Start SOURCE

./stop
sleep 1
/usr/bin/sensei -f /udata/source/app/source_sensei_config.json &
sleep 3
nohup ./main 1>/udata/source/app/source_start.log 2>/udata/source/app/source_start_error.log &
sleep 2
sushi -r --multicore-processing=2 -c /udata/source/app/source_sushi_config.json --log-level=debug &
sleep 3
aconnect -x
aconnect 16 128
